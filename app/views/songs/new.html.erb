<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <%= f.label :song_artist_name %>
  <%= f.text_field :song_artist_name, id: :song_artist_name %>
  <br />

  <%= f.label :genre %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br />

  <%= f.submit %>
<% end %>
